{
  "env": {
    "jest": true,
    "browser": true,
    "es6": true
  },
  "extends": ["standard", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "impliedStrict": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"]
  },
  "plugins": ["jest", "prettier"]
}
